    "@remix-run/dev": "^2.15.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.1.6",
    "vite": "^5.1.8",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.93.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
