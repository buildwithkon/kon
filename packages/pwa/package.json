{
  "name": "kon-pwa",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "pnpm run build && wrangler deploy",
    "deploy:staging": "pnpm run build && wrangler deploy --env staging",
    "dev": "remix vite:dev",
    "check": "biome check --write ./app",
    "lint": "biome lint ./app",
    "lint:unsafe": "biome lint --write --unsafe ./app",
    "format": "biome format --write ./app",
    "start": "wrangler dev --port=8788",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "pnpm run build && wrangler dev",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^0.35.6",
    "@coinbase/wallet-sdk": "^4.0.4",
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@dotmind/react-use-pwa": "^1.0.4",
    "@permissionless/wagmi": "^0.1.3",
    "@phosphor-icons/react": "^2.1.7",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@remix-pwa/sw": "3.0.10",
    "@remix-pwa/worker-runtime": "2.1.4",
    "@remix-run/cloudflare": "^2.15.0",
    "@remix-run/react": "^2.15.0",
    "@remix-run/server-runtime": "^2.15.0",
    "@tanstack/react-query": "^5.55.4",
    "@uidotdev/usehooks": "^2.4.1",
    "@useorbis/db-sdk": "0.0.60-alpha",
    "@wagmi/connectors": "^5.3.10",
    "@wagmi/core": "^2.14.6",
    "boring-avatars": "^1.11.2",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "hono": "^4.6.12",
    "isbot": "^4.1.0",
    "jotai": "^2.10.2",
    "kon-api": "workspace:*",
    "permissionless": "^0.2.17",
    "qrcode.react": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.7.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "vaul": "^0.9.2",
    "viem": "2.x",
    "wagmi": "^2.12.9",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
