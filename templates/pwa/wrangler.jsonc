{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "kon-template-pwa",
  "compatibility_date": "2025-02-24",
  "main": "./workers/app.ts",
  "assets": {
    "directory": "./build/client/"
  },
  "vars": {
    "ENVIRONMENT": "production"
  },
  "services": [
    {
      "binding": "API_WORKER",
      "service": "kon-api"
    }
  ],
  "placement": {
    "mode": "smart"
  },
  "observability": {
    "enabled": true
  }
}
